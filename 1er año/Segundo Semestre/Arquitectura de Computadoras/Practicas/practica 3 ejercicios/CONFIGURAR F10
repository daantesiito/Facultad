;CONFIGURAR F10

IMR EQU 21h
EOI EQU 20h
INT0 EQU 24h

ORG 3000h
SUBRUT: INC DL

        MOV AL, 20H
        OUT EOI, AL
        
        IRET

ORG 2000h
MOV AX, SUBRUT
MOV BX, 40
MOV [BX], AX

CLI

MOV AL, 11111110b
OUT IMR, AL

MOV AL, 10
OUT INT0, AL

STI

LOOPI: CMP DL, 5
       JZ FIN
       JMP LOOPI

FIN: HLT
END
