programa ejercicio7
areas
  ciudad:AreaC(1,1,100,100)
robots
  robot robot1
  comenzar
    derecha
    repetir 9
      mover
    repetir 3
      derecha
    repetir 99
      mover
      si (HayFlorEnLaBolsa)
        depositarFlor
  fin
variables
  R-info:robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin