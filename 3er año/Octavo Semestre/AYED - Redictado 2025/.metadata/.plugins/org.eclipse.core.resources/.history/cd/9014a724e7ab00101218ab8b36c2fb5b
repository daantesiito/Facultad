package parciales;

public class comparadorDeArbolesGenerales {

	public boolean esInferiorProfundo(GeneralTree<Integer> arbol1, GeneralTree<Integer> arbol2) {
		if (arbol1 == null || arbol1.isEmpty())
			return true;
		if (arbol2 == null || arbol2.isEmpty()) 
			return false;
		return recorrido(arbol1, arbol2);
	}
	
	private boolean recorrido(GeneralTree<Integer> arbol1, GeneralTree<Integer> arbol2) {
		Queue<GeneralTree<Integer>> cola1 = new Queue<GeneralTree<Integer>>();
		Queue<GeneralTree<Integer>> cola2 = new Queue<GeneralTree<Integer>>();
		cola1.enqueue(arbol1);
		cola1.enqueue(null);
		cola2.enqueue(arbol2);
		cola2.enqueue(null);
		while (!cola1.isEmpty() && !cola2.isEmpty()) {
			GeneralTree<Integer> dato1 = cola1.dequeue();
			GeneralTree<Integer> dato2 = cola2.dequeue();
			if (dato1 != null && dato1 != null) {
				for
			}
		}
	}
	
}
