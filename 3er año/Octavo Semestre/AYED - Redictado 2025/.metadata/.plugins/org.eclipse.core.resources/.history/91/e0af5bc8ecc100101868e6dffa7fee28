package tp5;

import java.util.LinkedList;
import java.util.List;

public class practicaBFS {
	
	public List<String> recorrido(Graph<String> grafo, String comienzo) {
		LinkedList<String> resultado = new LinkedList<String>();
		boolean[] marca = new boolean[grafo.getSize()];
		Vertex<String> vertice = grafo.search(comienzo);
		if (vertice != null) {
			bfs(grafo, vertice, marca, resultado);
		}
	}

}
